= content_for :main_header do
  %h1 New Instructor

= form_for(@instructor, :url => managers_instructors_path, :method => :post) do |f|
  %fieldset
    = render :partial => 'form', :locals => { :f => f }
    .actions
      = f.submit "Create Instructor", :class => 'btn primary'
      or
      = link_to 'Cancel', managers_instructors_path, :class => 'btn'
