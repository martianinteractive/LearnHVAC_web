= content_for :main_header do
  %h1= @instructor.name
  %h4 Edit

= form_for(@instructor, :url => managers_instructor_path(@instructor), :method => :put) do |f|
  %fieldset
    = render :partial => 'form', :locals => { :f => f }
    .actions
      = f.submit "Update", :class => 'btn primary'
      or
      = link_to 'Cancel', managers_instructors_path, :class => 'btn'
