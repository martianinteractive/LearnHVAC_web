#tabs
  %ul
    %li= link_to @master_scenario.name, admins_master_scenario_path(@master_scenario)
    %li.ui-tabs-selected= link_to "Revisions", admins_master_scenario_revisions_path(@master_scenario)
  
  %br
  %table.listing
    %thead
      %tr
        %th Version
        %th Version Description
        %th Updated at
    - @versions.each do |version|
      %tr
        %td= version.version
        %td= version.version_note.try(:description)
        %td= link_to version.updated_at.to_formatted_s(:long), admins_master_scenario_revision_path(@master_scenario, version.version)
  %br
  = will_paginate @versions

