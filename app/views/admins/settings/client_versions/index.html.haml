#tabs
  %ul
    %li= link_to "Settings", admins_settings_path
    %li.ui-tabs-selected= link_to "Client Versions", admins_settings_client_versions_path
    %li= link_to "New Client Version", new_admins_settings_client_version_path

  %br
  %table.listing
    %thead
      %tr
        %th Version
        %th URL
        %th Release Date
    %tbody
      - @client_versions.each do |version|
        %tr
          %td= link_to version.version, admins_settings_client_version_path(version)
          %td= link_to version.url, version.url
          %td= version.release_date
  %br
  = will_paginate @client_versions

