#tabs
  %ul
    %li= link_to "Instructor Scenarios", admins_scenarios_path
    %li= link_to "New Scenario", new_admins_scenario_path

  - form_for(@scenario, :url => admins_scenario_path(@scenario), :method => :put) do |f|
    = render :partial => "form", :locals => { :f => f }
    .actions
      = submit_tag "Update"
      &nbsp;or&nbsp;
      = link_to 'Cancel', :back
