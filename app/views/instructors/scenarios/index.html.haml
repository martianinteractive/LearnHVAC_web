%h2 Your Scenarios

%p
  = link_to 'New scenario', new_instructors_scenario_path

%hr

%table
  %tr
    %th Name
    %th Description
  - @scenarios.each do |scenario|
    %tr
      %td= link_to scenario.name, instructors_scenario_path(scenario)
      %td= scenario.description
%br
= will_paginate @scenarios

