
<table border="0" cellpadding="0" cellspacing="0">
	<tr >
		<td valign="middle"><img src="/images/icon_sysvars_45.png" alt = "System Variable Icon"/></td>
		<td width="10">&nbsp;</td>
		<td valign="bottom"><h1>Editing System Variable</h1></td>
	</tr>
</table>


<% form_for (@systemvariable) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= submit_tag 'Edit', :class=>'submitButton' %>
<% end %>

<br/><br/>

<%= link_to 'Show', :action => 'show', :id => @systemvariable %> |
<%= link_to 'Back', :action => 'list' %>


