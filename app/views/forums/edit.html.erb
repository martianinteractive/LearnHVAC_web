<div>	
	<div id="section_icon">
	  <%= image_tag "icon_scenarios_45.png", :alt => "Scenarios Icon" %>
	</div>
	<div id="section_title">
		<h1>Editing Forum</h1>
	</div>
</div>

<% form_for @forum, :url => forum_path(@forum) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= f.submit 'Update', :class => 'submitButton' %> or <%= link_to "cancel", forums_path %>
<% end %>